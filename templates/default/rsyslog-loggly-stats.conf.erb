#Load stats module
module(
  load="impstats"
  interval="<%= node['loggly']['rsyslog_stats']['interval'] %>"                         # how often to generate stats
  log.file="<%= node['loggly']['rsyslog_stats']['logfile'] %>"     # file to write those stats to
